{
  "name": "white-label",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios:app1": "react-native run-ios --scheme App1",
    "ios:app2": "react-native run-ios --scheme App2",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "pods": "cd ios && pod install && cd ..",
    "xcode": "cd ios && xed . && cd ..",
    "android:app1": "react-native run-android --appId com.br.app1 --mode app1Debug",
    "android:app2": "react-native run-android --appId com.br.app2 --mode app2Debug",
    "clean": "npx react-native-clean-project"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-native": "0.73.3",
    "react-native-bootsplash": "^5.4.0",
    "react-native-device-info": "^10.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.20",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.4",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-native-clean-project": "^4.0.3",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
